<!DOCTYPE html>
<html>
<head>
    <title>SSTaskTracking</title>
    <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
    <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
    <%= csrf_meta_tags %>

    <script>
        <% if user_signed_in? %>
            var current_user_content = {
                id: <%= current_user.id %>,
                first_name: "<%= current_user.first_name %>",
                last_name: "<%= current_user.last_name%>"
            };
        <% end %>
    </script>

    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
        <!-- build:css(.tmp) styles/main.css -->
        <link rel="stylesheet" href="styles/main.css">
        <!-- endbuild -->

        <!-- build:js scripts/vendor/modernizr.js -->
        <script src="bower_components/modernizr/modernizr.js"></script>
        <!-- endbuild -->
  
         <!-- build:js scripts/vendor.js -->
        <script src="bower_components/jquery/jquery.js"></script>

        <script src="bower_components/jquery/jq-ajax-setup.js"></script>

        <script src="bower_components/underscore/underscore.js"></script>
        <script src="bower_components/backbone/backbone.js"></script>
        <script src="bower_components/backbone/backbone-mediator.js"></script>
        <!-- endbuild -->
    

        <!-- build:js({.tmp,app}) scripts/main.js -->

        <script src="scripts/main.js"></script>
        <script src="scripts/templates.js"></script>

        <!-- module User -->
        <script src="scripts/User/UserModel.js"></script>
        <script src="scripts/User/UserModelView.js"></script>

        <!-- module Projects-->
        <script src="scripts/Projects/ProjectsModel.js"></script>
        <script src="scripts/Projects/ProjectsCollection.js"></script>
        <script src="scripts/Projects/ProjectsModelView.js"></script>
        <script src="scripts/Projects/ProjectsCollectionView.js"></script>

        <!-- module ProductBacklogStories-->
        <script src="scripts/ProductBacklogStories/ProductBacklogStoriesModel.js"></script>
        <script src="scripts/ProductBacklogStories/ProductBacklogStoriesCollection.js"></script>
        <script src="scripts/ProductBacklogStories/ProductBacklogStoriesModelView.js"></script>
        <script src="scripts/ProductBacklogStories/ProductBacklogStoriesCollectionView.js"></script>
         
        <!-- module SprintBacklogStories -->
        <script src="scripts/SprintBacklogStories/SprintBacklogStoriesModel.js"></script>
        <script src="scripts/SprintBacklogStories/SprintBacklogStoriesCollection.js"></script>
        <script src="scripts/SprintBacklogStories/SprintBacklogStoriesModelView.js"></script>
        <script src="scripts/SprintBacklogStories/SprintBacklogStoriesCollectionView.js"></script>

        <!-- module ScrumPage-->
        <script src="scripts/ScrumPage/ScrumPageModel.js"></script>
        <script src="scripts/ScrumPage/ScrumPageModelView.js"></script>

        <!-- module Projectinfo-->
        <script src="scripts/Projectinfo/ProjectinfoModel.js"></script>
        <script src="scripts/Projectinfo/ProjectinfoModelView.js"></script>

         <!-- module ScrumBoard-->
        <script src="scripts/ScrumBoard/ScrumBoardModel.js"></script>
        <script src="scripts/ScrumBoard/ScrumBoardCollection.js"></script>
        <script src="scripts/ScrumBoard/ScrumBoardModelView.js"></script>
        <script src="scripts/ScrumBoard/ScrumBoardCollectionView.js"></script>
         
        <!-- endbuild -->
        

</head>
<body>

<%= yield %>

</body>
</html>
